{
  "name": "beeline/yii2-pgsql-advisory-mutex",
  "description": "PostgreSQL transaction-level advisory locks based mutex for Yii2",
  "type": "yii2-extension",
  "keywords": ["yii2", "mutex", "postgresql", "advisory-locks", "distributed-lock"],
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Pavel Dubrovsky",
      "email": "antikillerxxl@gmail.com"
    }
  ],
  "require": {
    "php": "^8.4",
    "yiisoft/yii2": "^2.0.45",
    "ext-pgsql": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "Beeline\\PgsqlAdvisoryMutex\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Beeline\\PgsqlAdvisoryMutex\\Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
